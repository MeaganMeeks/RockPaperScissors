<Window x:Class="RockPaperScissors.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RockPaperScissors"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="RockButton" Content="ROCK" HorizontalAlignment="Left" Margin="30,137,0,0" VerticalAlignment="Top" Width="113" Click="RockButton_Click" Height="36"/>
        <Button x:Name="PaperButton" Content="PAPER" HorizontalAlignment="Left" Margin="194,137,0,0" VerticalAlignment="Top" Width="113" Click="PaperButton_Click" Height="36"/>
        <Button x:Name="ScissorsButton" Content="SCISSORS" HorizontalAlignment="Left" Margin="360,137,0,0" VerticalAlignment="Top" Width="113" Click="ScissorsButton_Click" Height="36"/>
    </Grid>
</Window>
